{
  "id": null,
  "uid": "triton-logs",
  "title": "Triton Inference Server Logs",
  "tags": ["triton", "logs", "mlops"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 0},
      "type": "logs",
      "title": "Triton Server Logs - All Levels",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{container=\"triton_server\"}",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false
      }
    },
    {
      "id": 2,
      "gridPos": {"h": 10, "w": 12, "x": 0, "y": 10},
      "type": "logs",
      "title": "Error Logs Only",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{container=\"triton_server\"} |~ \"ERROR|FATAL|E\\\\d{4}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 10, "w": 12, "x": 12, "y": 10},
      "type": "logs",
      "title": "Warning Logs",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{container=\"triton_server\"} |~ \"WARN|W\\\\d{4}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "type": "stat",
      "title": "Error Count (Last 5m)",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "sum(count_over_time({container=\"triton_server\"} |~ \"ERROR|FATAL|E\\\\d{4}\" [5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "type": "stat",
      "title": "Warning Count (Last 5m)",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "sum(count_over_time({container=\"triton_server\"} |~ \"WARN|W\\\\d{4}\" [5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "orange", "value": 20}
            ]
          }
        }
      }
    },
    {
      "id": 6,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
      "type": "bargauge",
      "title": "Log Levels Distribution (Last 1h)",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({container=\"triton_server\"} | json | __error__=\"\" [1h]))",
          "refId": "A",
          "legendFormat": "{{level}}"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "reduceOptions": {
          "values": false,
          "fields": "",
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 500}
            ]
          }
        }
      }
    },
    {
      "id": 7,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 36},
      "type": "logs",
      "title": "Model-Specific Logs (Filter by model name)",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "{container=\"triton_server\"} |~ \"model\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false
      }
    },
    {
      "id": 8,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 46},
      "type": "timeseries",
      "title": "Log Rate by Level",
      "datasource": "Loki",
      "targets": [
        {
          "expr": "sum by (level) (rate({container=\"triton_server\"} | json | __error__=\"\" [1m]))",
          "refId": "A",
          "legendFormat": "{{level}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["last", "mean", "max"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never",
            "spanNulls": true
          },
          "unit": "logs/s"
        }
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "templating": {
    "list": []
  }
}
